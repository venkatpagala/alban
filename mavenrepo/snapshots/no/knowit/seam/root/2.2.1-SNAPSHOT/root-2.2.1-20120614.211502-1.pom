                <implementation>jpaconfiguration</implementation>
              </component>
            </components>
            <componentProperties>
              <persistenceunit>${app.persistence.unit.name}</persistenceunit>
              <outputfilename>schema.ddl</outputfilename>
              <drop>true</drop>
              <create>true</create>
              <export>false</export>
              <format>true</format>
            </componentProperties>
          </configuration>
        </plugin>

