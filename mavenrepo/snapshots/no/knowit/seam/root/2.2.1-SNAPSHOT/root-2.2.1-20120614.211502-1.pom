          <configuration>
            <downloadSources>true</downloadSources>
            <downloadJavadocs>true</downloadJavadocs>
            <dependenciesAsLibraries>true</dependenciesAsLibraries>
            <useFullNames>false</useFullNames>
          </configuration>
        </plugin>
        
        <!-- spock -->
        <plugin>
          <!-- 
            Finds all Spock specifications in this project, and configures Maven to run them
            If this plugin is removed, Spock specifications will need to adhere to Maven's naming 
            conventions for test classes 
          -->
          <groupId>org.spockframework</groupId>
          <artifactId>spock-maven</artifactId>
          <version>${version.spock-groovy-1.7}</version>
          <!-- you need to specify a concrete version number here -->
          <executions>
            <execution>
              <goals>
                <goal>find-specs</goal>
              </goals>
            </execution>
          </executions>
        </plugin>

      </plugins>
    </pluginManagement>
    
  </build>

