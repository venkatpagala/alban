if [ -z "$JAVA_HOME" ]; then
{% if java_default_implementation == 'oracle' %}
  JAVA_HOME="{{ java_install_dir }}/oracle/{{ java_oracle_version_str_pkg }}"
{% endif %}
{% if java_default_implementation == 'openjdk' %}
  JAVA_HOME="{{ java_install_dir }}/openjdk/{{ java_openjdk_version_str_pkg }}"
{% endif %}
fi

PATH="${PATH}:${JAVA_HOME}/bin"

export PATH
export JAVA_HOME
