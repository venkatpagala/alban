<?xml version="1.0" encoding="UTF-8"?>
<view_content>
	<main_toolbar>
		<control id="Init" maskId="ProfilingAdminLocaleMask">
			<img src="images/icones/refresh-normal.gif" forstate="normal"/>
			<img src="images/icones/refresh-unavailable.gif" forstate="unavailable"/>
			<navigation name="profilingGroupLocale" type="standard" params=""/>
		</control>
		<toolbar_separator/>
		<control id="Save" maskId="ProfilingAdminLocaleMask">
			<img src="images/icones/confirm-normal.gif" forstate="normal"/>
			<img src="images/icones/confirm-unavailable.gif" forstate="unavailable"/>
			<navigation name="profilingGroupLocaleSaveConfig" type="standard" params=""/>
		</control>
		<toolbar_separator/>
		<control id="Remove" maskId="ProfilingAdminLocaleMask">
			<img src="images/icones/delete-normal.gif" forstate="normal"/>
			<img src="images/icones/delete-unavailable.gif" forstate="unavailable"/>
			<navigation name="profilingGroupLocaleRemoveConfig" type="standard" params=""/>
		</control>
	</main_toolbar>

	<fieldline>
		<control maskId="ProfilingAdminLocaleMask" id="LocaleChoice"/>
	</fieldline>
	
	<fieldline>
		<control maskId="ProfilingAdminLocaleMask" id="UsersGroupTable" width="615" height="200" gType="pagined" pageSize="5"/>
	</fieldline>
</view_content>