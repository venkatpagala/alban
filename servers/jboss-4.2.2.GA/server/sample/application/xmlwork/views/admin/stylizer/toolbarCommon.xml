<?xml version="1.0" encoding="UTF-8"?>
<view_content>

<main_toolbar id="toolbarStatic">
		<control id="toolbarCommon" maskId="StylizerMask">
			<img src="images/kiwi/stylecustomizer/toolbarProperties/common.gif" forstate="normal"/>
			<img forstate="unavailable" src="images/kiwi/stylecustomizer/toolbarProperties/common-unavailable.gif"/>
			<navigation name="stylizerSetContainerContent" type="standard" params="viewHref=views/admin/stylizer/toolbarCommon.xml;;containerId=fields.toolbar___0"/>			
		</control>
		<toolbar_separator/>
		<control id="toolbarText" maskId="StylizerMask">
			<img src="images/kiwi/stylecustomizer/toolbarProperties/text.gif" forstate="normal"/>
			<img forstate="unavailable" src="images/kiwi/stylecustomizer/toolbarProperties/text-unavailable.gif"/>
			<navigation name="stylizerSetContainerContent" type="standard" params="viewHref=views/admin/stylizer/toolbarText.xml;;containerId=fields.toolbar___0"/>			
		</control>
		<toolbar_separator/>
		<control id="toolbarGraphic" maskId="StylizerMask">
			<img src="images/kiwi/stylecustomizer/toolbarProperties/image.gif" forstate="normal"/>
			<img forstate="unavailable" src="images/kiwi/stylecustomizer/toolbarProperties/image-unavailable.gif"/>
			<navigation name="stylizerSetContainerContent" type="standard" params="viewHref=views/admin/stylizer/toolbarGraphic.xml;;containerId=fields.toolbar___0"/>			
		</control>
		<toolbar_separator/>
		<control id="toolbarBorder" maskId="StylizerMask">
			<img src="images/kiwi/stylecustomizer/toolbarProperties/border.gif" forstate="normal"/>
			<img forstate="unavailable" src="images/kiwi/stylecustomizer/toolbarProperties/border-unavailable.gif"/>
			<navigation name="stylizerSetContainerContent" type="standard" params="viewHref=views/admin/stylizer/toolbarBorder.xml;;containerId=fields.toolbar___0"/>			
		</control>
		<toolbar_separator/>
		<control id="toolbarScroll" maskId="StylizerMask">
			<img src="images/kiwi/stylecustomizer/toolbarProperties/scrollbar.gif" forstate="normal"/>
			<img forstate="unavailable" src="images/kiwi/stylecustomizer/toolbarProperties/scrollbar-unavailable.gif"/>
			<navigation name="stylizerSetContainerContent" type="standard" params="viewHref=views/admin/stylizer/toolbarScroll.xml;;containerId=fields.toolbar___0"/>			
		</control>
		<toolbar_separator/>
		<control id="toolbarNavigation" maskId="StylizerMask">
			<img src="images/kiwi/stylecustomizer/toolbarProperties/navigation.gif" forstate="normal"/>
			<img forstate="unavailable" src="images/kiwi/stylecustomizer/toolbarProperties/navigation-unavailable.gif"/>
			<navigation name="stylizerSetContainerContent" type="standard" params="viewHref=views/admin/stylizer/toolbarNavigation.xml;;containerId=fields.toolbar___0"/>			
		</control>		
</main_toolbar>
	
<fieldset noborder="true" width="760" height="300">

<header>
		<control id="labelToolbarCommon" maskId="StylizerMask"/>
</header>

<fieldline height="4"></fieldline>

<fieldline>
	<fieldset legend="common" legendBold="true" width="760" >
		<fieldline>
			<control id="cursor" maskId="StylizerMask"/>			
		</fieldline>
		<fieldline height="2"></fieldline>
		<fieldline>
			<control id="height" maskId="StylizerMask">
				<spinner validate="true"/>
			</control>
			<control id="width" maskId="StylizerMask">
				<spinner validate="true"/>
			</control>			
		</fieldline>
		<fieldline height="2"></fieldline>
		<fieldline>
			<control id="top" maskId="StylizerMask">
				<spinner validate="true"/>
			</control>
			<control id="bottom" maskId="StylizerMask">
				<spinner validate="true"/>
			</control>			
		</fieldline>
		<fieldline height="2"></fieldline>
		<fieldline>
			<control id="left" maskId="StylizerMask">
				<spinner validate="true"/>
			</control>
			<control id="right" maskId="StylizerMask">
				<spinner validate="true"/>
			</control>			
		</fieldline>
		<fieldline height="2"></fieldline>				
		<fieldline>
			<control id="visibility" maskId="StylizerMask"/>
			<control id="display" maskId="StylizerMask" gType="longComboBox"/>
		</fieldline>		
	</fieldset>
	</fieldline>
	
	<fieldline>
		<control id="stylename" maskId="StylizerMask"/>		
	</fieldline>

</fieldset>		
</view_content>