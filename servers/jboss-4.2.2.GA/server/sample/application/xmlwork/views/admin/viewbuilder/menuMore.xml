<?xml version="1.0" encoding="UTF-8"?>
<view_content> 
	<main_toolbar>		
		<group id="AddMenu" label="Add" type="menuToolbarButton">
			<control id="btOpenAddSubMenuInMenuMore" maskId="MaskCustomizer" gType="menuToolBarCommand">
				<img forstate="normal" src="images/icones/hierarchy-normal.gif"/>
				<navigation type="standard" name="openWindow" params="windowId=AddSubMenuInMenuMorePopup"/>
			</control>
			<control id="btOpenAddFieldInMenuMore" maskId="MaskCustomizer" gType="menuToolBarCommand">
				<img forstate="normal" src="images/icones/insert-normal.gif"/>
				<navigation type="standard" name="openWindow" params="windowId=AddFieldMenuMorePopup"/>
			</control>			
		</group>
		<toolbar_separator/>
		<control id="btRemoveFieldInMenuMore" maskId="MaskCustomizer">
			<img forstate="normal" src="images/icones/delete-normal.gif"/>
		 </control>   
		 <toolbar_separator/>
		 <control id="btUp" maskId="MaskCustomizer">
			<img forstate="normal" src="images/kiwi/builder/up-normal.gif"/>
		</control>
		 <toolbar_separator/>
		<control id="btDown" maskId="MaskCustomizer">
			<img forstate="normal" src="images/kiwi/builder/down-normal.gif"/>
		</control>
		<toolbar_separator/>
		<control id="btCloseMenuMore" maskId="MaskCustomizer">
			<navigation type="standard" name="closeWindow" params="windowId=menuMore"/>
		</control><toolbar_separator/>
	</main_toolbar>
 
  
  <control id="menuMoreTree" maskId="MaskCustomizer" position="10,10" height="350" width="300"/>	

  
</view_content>