		<container:tab id="modifyTab">
			<view:view>
				<view:title>Modify</view:title> 
				<view:body href="ToolBarModify.xml"/> 
			</view:view>
		</container:tab>
		<container:tab id="addTab">
			<view:view>
				<view:title>Add</view:title> 
				<view:body href="ToolBarAdd.xml"/> 
			</view:view>
