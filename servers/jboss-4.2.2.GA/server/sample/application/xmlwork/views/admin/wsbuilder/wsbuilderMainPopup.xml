<?xml version="1.0" encoding="UTF-8"?>
<view_content xmlns:container="http://www.rfs.com/fw30/container" xmlns:view="http://www.rfs.com/fw30/view"> 
	
	<main_toolbar>
		
		<control id="bcToolbarNewWorksheet" maskId="WSBuilderMask">
			<img src="images/icones/new-normal.gif" forstate="normal"/>
			<img forstate="unavailable" src="images/icones/new-unavailable.gif"/>
			<navigation name="wsbuilderChangeWSBuilderMainPopup" type="standard" params="title=New Worksheet;;href=views/admin/wsbuilder/wsbuilderNewPopup.xml;;width=550;;height=390"/>
		</control>
		
		<toolbar_separator/>
		
		<control id="bcToolbarOpenWorksheet" maskId="WSBuilderMask">
			<img src="images/icones/open-normal.gif" forstate="normal"/>
			<img forstate="unavailable" src="images/icones/open-unavailable.gif"/>
			<navigation name="wsbuilderChangeWSBuilderMainPopup" type="standard" params="title=Open Worksheet;;href=views/admin/wsbuilder/wsbuilderOpenPopup.xml;;width=555;;height=390"/>
		</control>
		
		<toolbar_separator/>
		
		<control id="bcToolbarCloseWorksheet" maskId="WSBuilderMask">
			<img src="images/icones/delete-normal.gif" forstate="normal"/>
			<img forstate="unavailable" src="images/icones/delete-unavailable.gif"/>
		</control>
		
		<toolbar_separator/>
		
		<control id="bcToolbarSaveWorksheet" maskId="WSBuilderMask">
			<img src="images/icones/save-normal.gif" forstate="normal"/>
			<img forstate="unavailable" src="images/icones/save-unavailable.gif"/>
		</control>
		
		<toolbar_separator/>
		
		<control id="bcToolbarCommentWorksheet" maskId="WSBuilderMask">
			<img src="images/icones/text-normal.gif" forstate="normal"/>
			<img forstate="unavailable" src="images/icones/text-unavailable.gif"/>
			<navigation name="wsbuilderOpenWorkSheetCommentEditor" type="standard" params=""/>
		</control>
		
		
	</main_toolbar>
	
	<container:transparent id="wsbuilderContentPopup" width="550" height="500">
		<view:view>
			<view:body href="views/admin/wsbuilder/wsbuilderCurrentPopup.xml"/>
		</view:view>
	</container:transparent>
	
</view_content>