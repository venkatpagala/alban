<?xml version="1.0" encoding="UTF-8"?>
<statusbar>
	
	<control id="connectionStatusChoice" maskId="reportProviderStatusMask">
		<mappings>
			<mapping value="67" img="reporton"/>
			<mapping value="68" img="reportoff"/>
		</mappings>
	</control>
	
	<statusbar_separator/>
	
	<control id="choice" maskId="BugStatusBarAsyncUpdate">
	<!--  <control id="sampleChoice" maskId="AsyncSampleMask" width="40px" gType="statusBarItem"> -->
		<navigation type="standard" name="openWindow" params="windowId=window.style.new"/>
		<mappings>
			<mapping value="1" img="statusgreen"/>
			<mapping value="2" img="statusred"/>
		</mappings>
	</control>
	
	<statusbar_separator/>
	
	<control id="string" maskId="BugStatusBarAsyncUpdate" width="300"/>
	
</statusbar>