<?xml version="1.0" encoding="UTF-8"?>
<view_content>
    <!-- The securityId is defined in class InMemorySecurityAcces -->
    <!-- Use this custom security factory rather than the TradeAcces one for testing.-->
    <toolbar>
        <!-- The securityId is defined in class InMemorySecurityAcces -->
        <!-- Use this custom security factory rather than the TradeAcces one for testing.-->
        
            
            
            
        <control maskId="CreditLimitCoverage" id="cmdInsert_Coverage" securityId="ReadWrite" img="insert"><navigation type="custom" name="com.rfs.ktpplus.navigation.NavigationAccreditation"/></control>
        <toolbar_separator/>

        <!-- Version TestKTP -> no Access and no modif -->
        
            
            
            
        <control maskId="CreditLimitCoverage" id="cmdInsert_Coverage" securityId="ReadOnly" img="insert"><navigation type="custom" name="com.rfs.ktpplus.navigation.NavigationAccreditation"/></control>
        <toolbar_separator/>

        <!-- Read only access -->
        
            
            
            
        <control maskId="CreditLimitCoverage" id="cmdInsert_Coverage" securityId="WriteOnly" img="insert"><navigation type="custom" name="com.rfs.ktpplus.navigation.NavigationAccreditation"/></control>
        <toolbar_separator/>

        
            
            
            
        <control maskId="CreditLimitCoverage" id="cmdInsert_Coverage" securityId="NoAccess" img="insert"><navigation type="custom" name="com.rfs.ktpplus.navigation.NavigationAccreditation"/></control>
        <toolbar_separator/>

        <!-- If the securityId is set to a security right that does not exist, it should use -->
        <!-- the property kiwi.security.defaultRight defined in the runtime props file . -->
        
            
            
            
        <control maskId="CreditLimitCoverage" id="cmdInsert_Coverage" securityId="A_SECURITY_RIGHT_that_does_not_exist" img="insert"><navigation type="custom" name="com.rfs.ktpplus.navigation.NavigationAccreditation"/></control>
        <toolbar_separator/>
    </toolbar>

</view_content>