<?xml version="1.0" encoding="UTF-8"?>
<worksheet version="1.0" name="NavigationBuilder" xmlns:container="http://www.rfs.com/fw30/container"
           xmlns:view="http://www.rfs.com/fw30/view" xmlns:layout="http://www.rfs.com/fw30/layout"
           xmlns:xi="http://www.w3.org/2001/XInclude">

    <main_toolbar>
        <control id="btAdd" maskId="NavigationBuilderMask">
            <img src="images/icones/load-normal.gif" forstate="normal"/>
            <img forstate="unavailable" src="images/icones/load-unavailable.gif"/>
        </control>
        <toolbar_separator/>
        <control id="btRemove" maskId="NavigationBuilderMask">
            <img src="images/icones/delete-normal.gif" forstate="normal"/>
            <img forstate="unavailable" src="images/icones/delete-unavailable.gif"/>
        </control>
        <toolbar_separator/>
        <control id="btSaveNavigationFile" maskId="NavigationBuilderMask">
            <img src="images/icones/save-as-normal.gif" forstate="normal"/>
            <img forstate="unavailable" src="images/icones/save-normal-unavailable.gif"/>
        </control>

    </main_toolbar>

	<layout:table>
		<layout:row>
			<layout:cell>
			    <container:transparent id="navigationbuilderContentPopup" width="800" height="600">
			        <view:view>
			            <view:body href="views/admin/navigationbuilder/navigationbuilderContentPopup.xml"/>
			        </view:view>
			    </container:transparent>
			</layout:cell>
		</layout:row>
	</layout:table>

</worksheet>