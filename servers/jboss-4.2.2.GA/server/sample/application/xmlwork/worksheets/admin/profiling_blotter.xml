<?xml version="1.0" encoding="UTF-8"?>
<worksheet version="1.0" name="profiling.blotter"
	xmlns:container="http://www.rfs.com/fw30/container"
	xmlns:view="http://www.rfs.com/fw30/view"
	xmlns:layout="http://www.rfs.com/fw30/layout">
	
	<layout:table>
		<layout:row>
			<layout:cell>
				<container:tabs id="profiling.blotter" height="385" width="910" selected="tab1">
					<container:tab id="tab1">
						<view:title>Export</view:title>
						<view:body href="views/profiling/profiling_export_blotter.xml"/>
					</container:tab>
					
					<container:tab id="tab2">
						<view:title>Manager</view:title>
						<view:body href="views/profiling/profiling_manager_blotter.xml"/>
					</container:tab>
				</container:tabs>
			</layout:cell>
		</layout:row>
	</layout:table>
	
	
	<container:window id="viewExportPopup" width="420" height="280" position="150,50" closed="true">
		<view:view>
			<view:title>XML Export Config</view:title>
			<view:body href="views/profiling/blotter/profiling_export_window_view.xml"/>
		</view:view>
	</container:window>
	
</worksheet>