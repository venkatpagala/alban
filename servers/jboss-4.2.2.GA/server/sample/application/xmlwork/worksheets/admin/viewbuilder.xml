			<img forstate="unavailable" src="images/icones/back-unavailable.gif"/>
		</control>
		<toolbar_separator/>
		<control id="btRedo" maskId="MaskCustomizer">
			<img src="images/icones/forward-normal.gif" forstate="normal"/>
			<img forstate="unavailable" src="images/icones/forward-unavailable.gif"/>
		</control>
