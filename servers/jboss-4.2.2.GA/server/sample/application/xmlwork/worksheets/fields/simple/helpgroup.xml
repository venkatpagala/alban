<?xml version="1.0" encoding="UTF-8"?>
<worksheet version="1.0" name="string" xmlns:container="http://www.rfs.com/fw30/container" xmlns:view="http://www.rfs.com/fw30/view"
	xmlns:layout="http://www.rfs.com/fw30/layout" xmlns:xi="http://www.w3.org/2001/XInclude">
	
	<main_toolbar>
		<control id="enable" maskId="HelpGroupSample"/>
		<toolbar_separator/>
		<control id="visible" maskId="HelpGroupSample"/>
		<toolbar_separator/>
		<control id="change" maskId="HelpGroupSample"/>
	</main_toolbar>
	
	<layout:table>
		<layout:row>
			<layout:cell>				
				<layout:table>
					<layout:row>
						<layout:cell>
							<container:static id="fields.helpgroup.helpgroup" width="600" height="60">
								<view:view>
									<view:title>HelpGroup</view:title>
									<view:body href="views/fields/helpgroup/helpgroup.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.helpgroup.helpgroup.short" width="600" height="60">
								<view:view>
									<view:title>HelpGroup (short)</view:title>
									<view:body href="views/fields/helpgroup/helpgroup_short.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.helpgroup.helpgroup.long" width="600" height="60">
								<view:view>
									<view:title>HelpGroup (long)</view:title>
									<view:body href="views/fields/helpgroup/helpgroup_long.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.helpgroup.helpgroup.test" width="600" height="80">
								<view:view>
									<view:title>Test Fields</view:title>
									<view:body href="views/fields/helpgroup/helpgroup_test.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.helpgroup.helpgroup.suggest" width="600" height="80">
								<view:view>
									<view:title>Helpgoup Suggest</view:title>
									<view:body href="views/fields/helpgroup/helpgroup_suggest.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.helpgroup.helpgroup.readOnly" width="600" height="80">
								<view:view>
									<view:title>Helpgoup (readOnly)</view:title>
									<view:body href="views/fields/helpgroup/helpgroup_readonly.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.helpgroup.helpgroup.longReadOnly" width="600" height="80">
								<view:view>
									<view:title>Helpgoup (longReadOnly)</view:title>
									<view:body href="views/fields/helpgroup/helpgroup_longreadonly.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
				</layout:table>
			</layout:cell>
			<layout:cell>				
				<container:tabs id="tabs.xml" height="450" width="600" selected="tabs.xml.tab1">
					<container:tab id="tabs.xml.tab1">
						<view:title>default</view:title>
						<view:body href="views/fields/helpgroup/helpgroupComment.xml"/>
					</container:tab>
					<container:tab id="tabs.xml.tab2">
						<view:title>short</view:title>
						<view:body href="views/fields/helpgroup/helpgroupComment_short.xml"/>
					</container:tab>
					<container:tab id="tabs.xml.tab3">
						<view:title>long</view:title>
						<view:body href="views/fields/helpgroup/helpgroupComment_long.xml"/>
					</container:tab>
					<container:tab id="tabs.xml.tab4">
						<view:title>suggest</view:title>
						<view:body href="views/fields/helpgroup/helpgroupComment_suggest.xml"/>
					</container:tab>
					<container:tab id="tabs.xml.tab5">
						<view:title>readOnly</view:title>
						<view:body href="views/fields/helpgroup/helpgroupComment_readonly.xml"/>
					</container:tab>
					<container:tab id="tabs.xml.tab6">
						<view:title>longReadOnly</view:title>
						<view:body href="views/fields/helpgroup/helpgroupComment_longreadonly.xml"/>
					</container:tab>
				</container:tabs>				
			</layout:cell>
		</layout:row>
	</layout:table>
	
</worksheet>