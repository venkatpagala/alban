<?xml version="1.0" encoding="UTF-8"?>
<worksheet version="1.0" name="double" xmlns:container="http://www.rfs.com/fw30/container" xmlns:view="http://www.rfs.com/fw30/view"
	xmlns:layout="http://www.rfs.com/fw30/layout" xmlns:xi="http://www.w3.org/2001/XInclude">
	
	<main_toolbar>
		<control id="enable" maskId="DoubleSample"/>
		<toolbar_separator/>
		<control id="visible" maskId="DoubleSample"/>
		<toolbar_separator/>
		<control id="nullify" maskId="DoubleSample"/>
		<toolbar_separator/>
		<control id="doubleOnlyTemplateSearch" maskId="TemplateMask" sizeInput="150"/>
