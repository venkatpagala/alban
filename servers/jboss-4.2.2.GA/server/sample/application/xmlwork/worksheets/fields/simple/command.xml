<?xml version="1.0" encoding="UTF-8"?>
<worksheet version="1.0" name="string" xmlns:container="http://www.rfs.com/fw30/container" xmlns:view="http://www.rfs.com/fw30/view" xmlns:layout="http://www.rfs.com/fw30/layout" xmlns:xi="http://www.w3.org/2001/XInclude">
	
	<main_toolbar>
		<control id="enable" maskId="CommandSample"/>
		<toolbar_separator/>
		<control id="visible" maskId="CommandSample"/>
	</main_toolbar>

	<layout:table>
		<layout:row>
			<layout:cell>
				<layout:table>
					<layout:row>
						<layout:cell>
							<container:static id="fields.command.command" width="600" height="60">
								<view:view>
									<view:title>Command</view:title>
									<view:body href="views/fields/command/command.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.command.command.link" width="600" height="60">
								<view:view>
									<view:title>Command (link)</view:title>
									<view:body href="views/fields/command/command_link.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.command.command.image" width="600" height="60">
								<view:view>
									<view:title>Image</view:title>
									<view:body href="views/fields/command/command_image.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
				</layout:table>
			</layout:cell>
			<layout:cell>				
				<container:tabs id="tabs.xml" height="450" width="600" selected="tabs.xml.tab1">
					<container:tab id="tabs.xml.tab1">
						<view:title>default</view:title>
						<view:body href="views/fields/command/commandComment.xml"/>
					</container:tab>
					<container:tab id="tabs.xml.tab2">
						<view:title>link</view:title>
						<view:body href="views/fields/command/commandComment_link.xml"/>
					</container:tab>
					<container:tab id="tabs.xml.tab3">
						<view:title>image</view:title>
						<view:body href="views/fields/command/commandComment_image.xml"/>
					</container:tab>
				</container:tabs>				
			</layout:cell>
		</layout:row>
	</layout:table>
	
</worksheet>