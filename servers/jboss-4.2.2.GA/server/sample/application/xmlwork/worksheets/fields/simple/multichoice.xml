<?xml version="1.0" encoding="UTF-8"?>
<worksheet version="1.0" name="choice" xmlns:container="http://www.rfs.com/fw30/container" xmlns:view="http://www.rfs.com/fw30/view"
	xmlns:layout="http://www.rfs.com/fw30/layout" xmlns:xi="http://www.w3.org/2001/XInclude">
	
	<main_toolbar>
		<control id="enable" maskId="MultiChoiceSample"/>
		<toolbar_separator/>
		<control id="visible" maskId="MultiChoiceSample"/>
	</main_toolbar>
	
	<layout:table>
		<layout:row>
			<layout:cell>				
				<container:static id="fields.multichoice.multichoice" width="600" height="100">
					<view:view>
						<view:title>MultiChoice (default)</view:title>
						<view:body href="views/fields/multichoice/multichoice.xml"/>
					</view:view>
				</container:static>				
			</layout:cell>
			<layout:cell>				
				<container:tabs id="tabs.xml" height="450" width="600" selected="tabs.xml.tab1">
					<container:tab id="tabs.xml.tab1">
						<view:title>default</view:title>
						<view:body href="views/fields/multichoice/multichoiceComment.xml"/>
					</container:tab>
				</container:tabs>				
			</layout:cell>
		</layout:row>
	</layout:table>
	
</worksheet>