<?xml version="1.0" encoding="UTF-8"?>
<worksheet version="1.0" name="string" xmlns:container="http://www.rfs.com/fw30/container" xmlns:view="http://www.rfs.com/fw30/view" xmlns:layout="http://www.rfs.com/fw30/layout" xmlns:xi="http://www.w3.org/2001/XInclude">
	
	<!--Choice -->
	<main_toolbar id="toolbarLive">
			        
		<control id="choice" maskId="ChoiceSample" gType="comboBox"/>
        <toolbar_separator/>
        <control id="choiceLong" maskId="ChoiceSample" gType="longComboBox"/>
               
    </main_toolbar>
    
    <main_toolbar id="toolbarLive">
    	<control id="choiceRadioButton" maskId="ChoiceSample" gType="radioButton"/>
    	<toolbar_separator/>
    	<control id="choiceRadioButtonLong" maskId="ChoiceSample" gType="radioButtonLong"/>    	
    </main_toolbar>
                  
    <main_toolbar id="toolbarLive">
    	<control id="choiceRadioButtonBloc" maskId="ChoiceSample" gType="radioButtonBloc"/>
    </main_toolbar>
    
    <main_toolbar id="toolbarLive">
    	<control id="choiceRadioButtonColumn" maskId="ChoiceSample" gType="radioButtonColumn"/>
    		<toolbar_separator/>
    	<control id="choiceRadioButtonLine" maskId="ChoiceSample" gType="radioButtonLine"/>
    </main_toolbar>
    
    <main_toolbar id="toolbarLive">    
    	<control id="choiceCheckBox" maskId="ChoiceSample" gType="checkBox" sizeInput="70"/>     
    	<toolbar_separator/>
    	<control id="choiceCheckBoxLongText" maskId="ChoiceSample" gType="checkBox" sizeInput="100"/>
    	<toolbar_separator/>
    	<control id="choiceCheckBoxNoLabel" maskId="ChoiceSample" gType="checkBox" nolabel="true"/>
    	<toolbar_separator/>
    	<control id="choiceCheckBoxNoLabelLongText" maskId="ChoiceSample" gType="checkBox" nolabel="true" fixedSizeLabel="false"/>
    </main_toolbar>
    
    
    <!--Command -->
    
    <main_toolbar id="toolbarLive">		        
		
        <control id="commandLink" maskId="CommandSample" gType="link"/>
		<toolbar_separator/>
		
			
			
		<control id="commandLinkImage" maskId="CommandSample" gType="link" img="clear"></control>
        
                       
    </main_toolbar>
                
    <!--Help group -->

	<main_toolbar id="toolbarLive">		        

		<control id="helpgroup" maskId="HelpGroupSample"/>
        <toolbar_separator/>
        <control id="helpgroupShort" maskId="HelpGroupSample" gType="short"/>            
                        
    </main_toolbar>
    
    <main_toolbar id="toolbarLive">        
        <control id="helpgroupLong" maskId="HelpGroupSample" gType="long"/>    
    </main_toolbar>    	
    
    <!--Progress bar -->

	<main_toolbar id="toolbarLive">		        

		<control id="staticProgressBar" maskId="ProgressBarSample"/> 
		<toolbar_separator/>       
        <control id="dynamicProgressBar" maskId="ProgressBarSample"/>
    </main_toolbar>
    
    <!--String -->

	<main_toolbar id="toolbarLive">		        

     <control id="string" maskId="StringSample"/>
     <toolbar_separator/>	
     <control id="stringLong" maskId="StringSample" gType="long"/>     
        
    </main_toolbar>
    
    
     <!--Number-->
     
    <main_toolbar id="toolbarLive">		        

     
		
	<control id="doubleSpinner" maskId="DoubleSample"><spinner validate="true"/></control>
		
	 <toolbar_separator/>
	 	 
	 <control id="doubleLeftAndRightBound" maskId="DoubleSample"/>
	 
	 <toolbar_separator/>
	 
	 
		
	<control id="doubleLongUnit" maskId="DoubleSample" longUnit="true" sizeUnit="100"><spinner validate="true"/></control>	
               
    </main_toolbar>
    
    
        
    <main_toolbar id="toolbarLive">		        
	
	<control id="doubleReadOnly" maskId="DoubleSample" gType="readOnly"/>     
	 <toolbar_separator/>
	 <control id="doubleReadOnlyNoBorder" maskId="DoubleSample" gType="readOnlyNoBorder"/>
	 
	               
    </main_toolbar>
    
    <!--Color Picker-->
    <main_toolbar id="toolbarLive">
    	<control id="colorPicker" maskId="ColorPickerSample"/>
    </main_toolbar>
    
     <!--Date -->
    <main_toolbar id="toolbarLive">
    	<control id="date" maskId="DateSample"/>
    	<toolbar_separator/>
    	<control id="dateReadOnly" maskId="DateSample" gType="readOnly"/>
    	<toolbar_separator/>
    	<control id="dateReadOnlyNoBorder" maskId="DateSample" gType="readOnlyNoBorder"/>
    	<toolbar_separator/>
    	
		
		<control id="dateSpinner" maskId="DateSample"><spinner/></control>					
    </main_toolbar>
    
	<!--kpls680 et kpls683 - fields are aligned-->
    <main_toolbar id="toolbarLive">
		
			
			
		<control id="commandImage" maskId="CommandSample" img="clear"></control>		
		<toolbar_separator/>
		<control id="helpgroupLong" maskId="HelpGroupSample" gType="long"/>
		<toolbar_separator/>
		
			
		<control id="intLongUnit" maskId="IntSample" longUnit="true" sizeUnit="65"><spinner validate="true"/></control>
		<toolbar_separator/>	
		<control id="string" maskId="StringSample"/>
		<toolbar_separator/>
		<control id="floatUnit" maskId="FloatSample"/>
	</main_toolbar>
	
    <main_toolbar>
		<control id="uploadFileTest" maskId="UploadFileSample" label="Upload a report"/>
		<control id="uploadFileImage" maskId="UploadFileSample" label="Upload a file" img="simulate"></control>
	</main_toolbar>
    
</worksheet>