<?xml version="1.0" encoding="UTF-8"?>
<worksheet version="1.0" name="date" xmlns:container="http://www.rfs.com/fw30/container" xmlns:view="http://www.rfs.com/fw30/view"
	xmlns:layout="http://www.rfs.com/fw30/layout" xmlns:xi="http://www.w3.org/2001/XInclude">
	
	<main_toolbar>
		<control id="enable" maskId="DateSample"/>
		<toolbar_separator/>
		<control id="visible" maskId="DateSample"/>
	</main_toolbar>
	
	<layout:table>
		<layout:row>
			<layout:cell>
				<layout:table>
					<layout:row>
						<layout:cell>
							<container:static id="fields.date.date" width="600" height="70">
								<view:view>
									<view:title>Date</view:title>
									<view:body href="views/fields/date/date.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.date.date.readOnly" width="600" height="70">
								<view:view>
									<view:title>Date (readOnly)</view:title>
									<view:body href="views/fields/date/date_readOnly.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.date.date.readOnlyNoBorder" width="600" height="70">
								<view:view>
									<view:title>Date (readOnlyNoBorder)</view:title>
									<view:body href="views/fields/date/date_readOnlyNoBorder.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.date.date.header" width="600" height="70">
								<view:view>
									<view:title>Date (header)</view:title>
									<view:body href="views/fields/date/date_header.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.date.date.ampmtime" width="600" height="70">
								<view:view>
									<view:title>Date AM/PM Time</view:title>
									<view:body href="views/fields/date/date_timeAmPm.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.date.date.spinner" width="600" height="70">
								<view:view>
									<view:title>Spinner</view:title>
									<view:body href="views/fields/date/date_spinner.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.date.date.time" width="600" height="70">
								<view:view>
									<view:title>TimeOnly</view:title>
									<view:body href="views/fields/date/date_time.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.date.date.leftBound" width="600" height="70">
								<view:view>
									<view:title>Left Bound</view:title>
									<view:body href="views/fields/date/date_leftBound.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.date.date.rightBound" width="600" height="70">
								<view:view>
									<view:title>Right Bound</view:title>
									<view:body href="views/fields/date/date_rightBound.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.date.date.leftAndRightBound" width="600" height="70">
								<view:view>
									<view:title>Left and Right Bounds</view:title>
									<view:body href="views/fields/date/date_leftAndRightBound.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:static id="fields.date.date.timePreset" width="600" height="70">
								<view:view>
									<view:title>Time Preset</view:title>
									<view:body href="views/fields/date/date_timePreset.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
				</layout:table>
			</layout:cell>
			<layout:cell>				
				<container:tabs id="tabs.xml" height="450" width="600" selected="tabs.xml.tab1">
					<container:tab id="tabs.xml.tab1">
						<view:title>default</view:title>
						<view:body href="views/fields/date/dateComment.xml"/>
					</container:tab>
					<container:tab id="tabs.xml.tab2">
						<view:title>readOnly</view:title>
						<view:body href="views/fields/date/dateComment_readOnly.xml"/>
					</container:tab>
					<container:tab id="tabs.xml.tab3">
						<view:title>readOnlyNoBorder</view:title>
						<view:body href="views/fields/date/dateComment_readOnlyNoBorder.xml"/>
					</container:tab>
					<container:tab id="tabs.xml.tab4">
						<view:title>header</view:title>
						<view:body href="views/fields/date/dateComment_header.xml"/>
					</container:tab>
					<container:tab id="tabs.xml.tab5">
						<view:title>spinner</view:title>
						<view:body href="views/fields/date/dateComment_spinner.xml"/>
					</container:tab>
					<container:tab id="tabs.xml.tab6">
						<view:title>leftBound</view:title>
						<view:body href="views/fields/date/dateComment_leftBound.xml"/>
					</container:tab>
					<container:tab id="tabs.xml.tab7">
						<view:title>rightBound</view:title>
						<view:body href="views/fields/date/dateComment_rightBound.xml"/>
					</container:tab>
					<container:tab id="tabs.xml.tab8">
						<view:title>leftAndRightBound</view:title>
						<view:body href="views/fields/date/dateComment_leftAndRightBound.xml"/>
					</container:tab>
				</container:tabs>				
			</layout:cell>
		</layout:row>
	</layout:table>
	
</worksheet>