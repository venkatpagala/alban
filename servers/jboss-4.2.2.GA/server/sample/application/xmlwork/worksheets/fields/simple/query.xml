<?xml version="1.0" encoding="UTF-8"?>
<worksheet version="1.0" name="chart" xmlns:container="http://www.rfs.com/fw30/container" xmlns:view="http://www.rfs.com/fw30/view"
	xmlns:layout="http://www.rfs.com/fw30/layout">
	
	<query_navigation id="query" maskId="QuerySample">
		<navigation key="1" type="standard" name="openWindow" params="windowId=query.window.1"/>
		<navigation key="2" type="standard" name="openWindow" params="windowId=query.window.2"/>
	</query_navigation>
	
	<main_toolbar>
		<control id="visible1" maskId="QuerySample"/>		
		<control id="visible2" maskId="QuerySample" img="clear"/>
		<toolbar_separator/>	
		<control id="message1" maskId="QuerySample"/>
		<control id="message2" maskId="QuerySample"/>
		<control id="message3" maskId="QuerySample"/>
		<toolbar_separator/>	
		<control id="showQueryAndNavigate" maskId="QuerySample"/>
	</main_toolbar>
	
	<layout:table>
		<layout:row>
			<layout:cell>				
				<container:transparent id="fields.query.query" width="600" height="300">
					<view:view>
						<view:body href="views/fields/query/query.xml"/>
					</view:view>
				</container:transparent>				
			</layout:cell>
			<layout:cell>				
				<container:tabs id="tabs.xml" height="450" width="600" selected="tabs.xml.tab1">
					<container:tab id="tabs.xml.tab1">
						<view:title>default</view:title>
						<view:body href="views/fields/query/queryComment.xml"/>
					</container:tab>
				</container:tabs>				
			</layout:cell>
		</layout:row>
	</layout:table>
	
	<container:window id="query.window.1" height="200" width="400" position="50,50" img="images/uaf/Home-12-Normal.gif" closed="true">
		<view:view>
			<view:title>Window For Choice 1</view:title>
			<view:body href="views/container/commonFields.xml"/>
		</view:view>
	</container:window>
	
	<container:window id="query.window.2" height="200" width="400" position="50,50" img="images/uaf/Home-12-Normal.gif" closed="true">
		<view:view>
			<view:title>Window For Choice 2</view:title>
			<view:body href="views/container/commonFields.xml"/>
		</view:view>
	</container:window>
	
</worksheet>