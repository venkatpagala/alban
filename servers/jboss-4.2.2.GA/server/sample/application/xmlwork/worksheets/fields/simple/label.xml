<?xml version="1.0" encoding="UTF-8"?>
<worksheet version="1.0" name="label" xmlns:container="http://www.rfs.com/fw30/container" xmlns:view="http://www.rfs.com/fw30/view"
	xmlns:layout="http://www.rfs.com/fw30/layout" xmlns:xi="http://www.w3.org/2001/XInclude">
	
	<main_toolbar>
		<control id="enable" maskId="LabelSample"/>
		<toolbar_separator/>
		<control id="visible" maskId="LabelSample"/>
	</main_toolbar>
	
	<layout:table>
		<layout:row>
			<layout:cell>				
				<layout:table>
					<layout:row>
						<layout:cell>
							<container:static id="fields.label.label" width="600" height="60">
								<view:view>
									<view:title>Label</view:title>
									<view:body href="views/fields/label/label.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>							
							<container:static id="fields.label.label.header" width="600" height="60">
								<view:view>
									<view:title>Label (header)</view:title>
									<view:body href="views/fields/label/label_header.xml"/>
								</view:view>
							</container:static>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>							
							<container:static id="fields.label.label.color" width="600" height="150">
								<view:view>
									<view:title>Label (color)</view:title>
									<view:body href="views/fields/label/label_color.xml"/>
								</view:view>
							</container:static>							
						</layout:cell>
					</layout:row>
				</layout:table>
			</layout:cell>
			<layout:cell>
				<container:tabs id="tabs.xml" height="450" width="600" selected="tabs.xml.tab1">
					<container:tab id="tabs.xml.tab1">
						<view:title>default</view:title>
						<view:body href="views/fields/label/labelComment.xml"/>
					</container:tab>
					<container:tab id="tabs.xml.tab2">
						<view:title>header</view:title>
						<view:body href="views/fields/label/labelComment_header.xml"/>
					</container:tab>
				</container:tabs>				
			</layout:cell>
		</layout:row>
	</layout:table>
	
</worksheet>