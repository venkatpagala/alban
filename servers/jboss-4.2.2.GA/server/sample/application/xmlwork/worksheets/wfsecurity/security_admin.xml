<?xml version="1.0" encoding="UTF-8"?>
<worksheet version="1.0" name="WSSecurityAdmin" xmlns:container="http://www.rfs.com/fw30/container" xmlns:view="http://www.rfs.com/fw30/view" xmlns:layout="http://www.rfs.com/fw30/layout" xmlns:xi="http://www.w3.org/2001/XInclude">	
	<main_toolbar>
		<control id="SwitchElement" maskId="SecurityAdminMask"/>
		<toolbar_separator/>
		<control id="CreateProfile" maskId="SecurityAdminMask">
			<navigation name="openWindow" params="windowId=SecurityCreateProfileWindow" type="standard"/>
		</control>
		<toolbar_separator/>
		<control id="RemoveProfile" maskId="SecurityAdminMask"/>
		<toolbar_separator/>
		<control id="EditProfile" maskId="SecurityAdminMask">
			<navigation name="openWindow" params="windowId=SecurityAdminEditProfileWindow" type="standard"/>
		</control>
	</main_toolbar>

	<layout:table>
		<layout:row>
			<layout:cell>
				<container:transparent id="SecurityAdmin" height="500" width="2000">
					<view:view>
						<view:body href="views/wfsecurity/security_admin.xml"/>
					</view:view>
				</container:transparent>
			</layout:cell>
		</layout:row>
	</layout:table>

	<container:window id="SecurityAdminEditProfileWindow" height="300" width="500" position="280,130" closed="true">
		<view:view>
			<view:title>Edit Profile</view:title>
			<view:body href="views/wfsecurity/security_admin_edit_profile.xml"/>
		</view:view>
	</container:window>

	<container:window id="SecurityCreateElementWindow" height="30" width="500" position="280,130" closed="true">
		<view:view>
			<view:title>Create new element</view:title>
			<view:body href="views/wfsecurity/security_admin_create_element.xml"/>
		</view:view>
	</container:window>

	<container:window id="SecurityCreateProfileWindow" height="30" width="500" position="280,130" closed="true">
		<view:view>
			<view:title>Create new profile</view:title>
			<view:body href="views/wfsecurity/security_admin_create_profile.xml"/>
		</view:view>
	</container:window>
</worksheet>