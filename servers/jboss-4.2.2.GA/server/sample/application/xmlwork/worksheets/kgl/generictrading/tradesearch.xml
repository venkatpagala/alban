<?xml version="1.0" encoding="UTF-8"?>
<worksheet version="1.0" name="Trade Entry Worksheet" xmlns:container="http://www.rfs.com/fw30/container" xmlns:view="http://www.rfs.com/fw30/view" xmlns:layout="http://www.rfs.com/fw30/layout" xmlns:xi="http://www.w3.org/2001/XInclude">



	<!--worksheetDictionary name="GenericEnquiry" priority="true"/-->

	<layout:table>
		<layout:row>
			<layout:cell>
				<container:leftnavigation id="leftNav" height="300" width="230">
					<view:view>
						<view:title>Test Left Navigation</view:title>
						<view:body href="views/kgl/generictrading/leftnavigation.xml"/>
					</view:view>
				</container:leftnavigation>
				<container:expandable id="Messages" height="200" width="230" collapsed="true">
					<view:view>
						<view:title>Messages</view:title>
						<view:body href="views/kgl/generictrading/messages.xml"/>
					</view:view>
				</container:expandable>
				<container:expandable id="Search" height="200" width="230" collapsed="true">
					<view:view>
						<view:title>Search</view:title>
						<view:body href="views/kgl/generictrading/genericsearch/search_Folder.xml"/>
					</view:view>
				</container:expandable>
			</layout:cell>
			<layout:cell>
				<container:tabs id="trade.search.tabs" height="400" width="750" selected="search.criteria">
					<container:tab id="search.criteria" height="300" width="750"> 
						<view:view>
							<view:title>Criteria</view:title>
							<view:body href="views/kgl/generictrading/tradesearch/tradesearchcriteria.xml"/>
						</view:view>
					</container:tab>
					<container:tab id="search.result" height="300" width="750"> 
						<view:view>
							<view:title>Result</view:title>
							<view:body href="views/kgl/generictrading/tradesearch/tradesearchresult.xml"/>
						</view:view>
					</container:tab>
				</container:tabs>
			</layout:cell>
		</layout:row>
	</layout:table>
</worksheet>