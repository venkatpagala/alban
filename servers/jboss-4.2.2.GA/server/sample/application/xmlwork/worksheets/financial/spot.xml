<?xml version="1.0" encoding="UTF-8"?>
<worksheet version="1.0" xmlns:container="http://www.rfs.com/fw30/container" xmlns:view="http://www.rfs.com/fw30/view" xmlns:layout="http://www.rfs.com/fw30/layout" xmlns:xi="http://www.w3.org/2001/XInclude">
	
	<main_toolbar>
		<control id="quickSearch" maskId="SpotDealMask"/>
		<toolbar_separator/>	
		
			
			
		<control id="clear" maskId="SpotDealMask" img="clear"></control>
		
			
			
		<control id="insert" maskId="SpotDealMask" img="insert"></control>
		
			
			
		<control id="update" maskId="SpotDealMask" img="update"></control>
		
			
			
		<control id="delete" maskId="SpotDealMask" img="delete"></control>
		<toolbar_separator/>	
		<control id="spotDealTemplateSearch" maskId="TemplateMask" sizeInput="150"/>
		<control id="spotDealTemplateEdit" maskId="TemplateMask"/>
		<toolbar_separator/>	
		<control id="openStaticDataInNewWindow" maskId="SpotDealMask" gType="checkBox" sizeLabel="0"/>
		<toolbar_separator/>
		<control id="compareWithDeal" maskId="SpotDealMask"/>
	</main_toolbar>
	
	<header>
		<control id="deal" maskId="SpotDealMask"/>
		<control id="blockNumber" maskId="SpotDealMask"/>
		<control id="captureDate" maskId="SpotDealMask"/>
		<control id="user" maskId="SpotDealMask"/>
		<control id="hierarchy" maskId="SpotDealMask"/>
		<control id="typeOfEvent" maskId="SpotDealMask"/>
	</header>
	
	<layout:table>
		<layout:row>
			<layout:cell>
				<container:expandable height="100" id="spot_staticdataId" width="560">
					<view:view>
						<view:title>Static Data</view:title>
						<view:body href="views/financial/spot/staticdata.xml"/>
					</view:view>
				</container:expandable>
			</layout:cell>
		</layout:row>
		<layout:row>
			<layout:cell>
				<container:expandable height="200" id="spot_characteristicsId" width="560">
					<view:view>
						<view:title>Characteristics</view:title>
						<view:body href="views/financial/spot/characteristics.xml"/>
					</view:view>
				</container:expandable>
			</layout:cell>
		</layout:row>
	</layout:table>
				
</worksheet>