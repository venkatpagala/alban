<?xml version="1.0" encoding="UTF-8"?>
<worksheet version="1.0" name="MoreMenu" xmlns:container="http://www.rfs.com/fw30/container"
	xmlns:view="http://www.rfs.com/fw30/view" xmlns:layout="http://www.rfs.com/fw30/layout" xmlns:xi="http://www.w3.org/2001/XInclude">

	<main_toolbar>
		<control gType="toolBarCommand" id="SpotDeals@~Buttons@~QSearch" maskId="Spot Deals Mask" />


		<control gType="toolBarCommand" maskId="Spot Deals Mask" id="SpotDeals@~Buttons@~Insert"
			img="insert"></control>
		<group type="menuToolbarButton" label="MoreMenu1" id="moreMenu1">


			<control gType="menuToolBarCommand" maskId="Spot Deals Mask" id="SpotDeals@~Buttons@~Update"
				img="refresh"></control>
			<separator />


			<control gType="menuToolBarCommand" maskId="Spot Deals Mask" id="SpotDeals@~Buttons@~Delete"
				img="delete"></control>
		</group>
		<group type="menuToolbarButton" label="MoreMenu2" id="moreMenu2">
			<submenu img="images/icones/Audit-Normal.gif" label="SubMenu 1">



				<control gType="menuToolBarCommand" maskId="Spot Deals Mask" id="SpotDeals@~Buttons@~TextInfo"
					img="text">
					<navigation type="standard" name="openWindow" params="windowId=textinfo" />
				</control>
			</submenu>
			<submenu img="images/icones/Audit-Normal.gif" label="SubMenu 2">



				<control gType="menuToolBarCommand" maskId="Spot Deals Mask" id="SpotDeals@~Buttons@~AllocationCmd"
					img="allocation">
					<navigation type="standard" name="openWindow" params="windowId=spotdealallocation" />
				</control>
				<separator />
				<submenu gType="menuToolBarCommand" img="images/icones/Audit-Normal.gif" label="SubMenu Sample 1">


					<control gType="menuToolBarCommand" maskId="Spot Deals Mask" id="SpotDeals@~Buttons@~PrintDuplicate"
						img="print"></control>



					<control gType="menuToolBarCommand" maskId="Spot Deals Mask" id="SpotDeals@~Audit@~AuditButton"
						img="audit">
						<navigation type="standard" name="openWindow" params="windowId=spotdealaudit" />
					</control>
				</submenu>
			</submenu>
			<submenu img="audit" label="SubMenu 3">

				<control gType="menuToolBarCommand" maskId="Spot Deals Mask" id="SpotDeals@~Buttons@~StrategiesCmd"
					img="strategy">
					<navigation type="standard" name="openWindow" params="windowId=spotdealstrategies" />
				</control>
				<separator />
				<control gType="menuToolBarCommand" maskId="Spot Deals Mask" id="SpotDeals@~Buttons@~StrategiesOverviewCmd"
					img="strategy">
					<navigation type="standard" name="openWindow" params="windowId=spotdealstrategiesoverview" />
				</control>
			</submenu>
			<submenu label="SubMenu 4" img="audit">
				<control gType="menuToolBarCommand" maskId="Spot Deals Mask" id="SpotDeals@~Buttons@~Clear"
					img="clear" />
			</submenu>



		</group>

	</main_toolbar>

</worksheet>