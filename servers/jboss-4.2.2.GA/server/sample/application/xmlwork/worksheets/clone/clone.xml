<?xml version="1.0" encoding="UTF-8"?>
<worksheet version="1.0" name="Algebra Calculator" xmlns:container="http://www.rfs.com/fw30/container" xmlns:view="http://www.rfs.com/fw30/view" xmlns:layout="http://www.rfs.com/fw30/layout" xmlns:xi="http://www.w3.org/2001/XInclude">
	
	<main_toolbar>
		
			
		<control id="command1" maskId="CloneSample"><navigation type="standard" name="cloneWorkSheet"/></control>	
		<toolbar_separator/>
		
			
		<control id="command2" maskId="CloneSample"><navigation type="standard" name="openWindow" params="windowId=clone.window"/></control>
		<toolbar_separator/>
		<control id="command3" maskId="CloneSample"/>	
	</main_toolbar>
	
	<layout:table>
		<layout:row>
			<layout:cell>
							
				<container:expandable id="clone.expandable1" width="300" height="150">
					<view:view>
						<view:title>Expandable 1</view:title>
						<view:body href="views/clone/clone1.xml"/>
					</view:view>
				</container:expandable>
								
			</layout:cell>
		</layout:row>
		<layout:row>
			<layout:cell>
				
				<container:expandable id="clone.expandable2" width="300" height="150">
					<view:view>
						<view:title>Expandable 2</view:title>
						<view:body href="views/clone/clone2.xml"/>
					</view:view>								
				</container:expandable>
				
			</layout:cell>
		</layout:row>
	</layout:table>
	
	<container:window id="clone.window" width="300" height="150" closed="true" position="100,100">
		<view:view>
			<view:title>Window</view:title>
			<view:body href="views/clone/clone3.xml"/>
		</view:view>
	</container:window>
				
</worksheet>