<?xml version="1.0" encoding="UTF-8"?>
<worksheet version="1.0" name="Expandable" xmlns:container="http://www.rfs.com/fw30/container"
	xmlns:view="http://www.rfs.com/fw30/view" xmlns:layout="http://www.rfs.com/fw30/layout">

	<layout:table>
		<layout:row>
			<layout:cell>
				<layout:table>
					<layout:row>
						<layout:cell>
							<xml_sample id="sample" xmlns:container="http://www.rfs.com/fw30/container"
								xmlns:view="http://www.rfs.com/fw30/view" xmlns:layout="http://www.rfs.com/fw30/layout">
								<container:expandable id="containers.expandable" height="300" width="600">
									<view:view>
										<view:title>Expandable Container</view:title>
										<view:body href="views/container/commonFields.xml" />
									</view:view>
								</container:expandable>
							</xml_sample>
						</layout:cell>
					</layout:row>
					<layout:row>
						<layout:cell>
							<container:expandable id="containers.expandable.light" height="300" width="600" light="true">
								<view:view>
									<view:title>Expandable Container Light</view:title>
									<view:body href="views/container/commonFields.xml" />
								</view:view>
							</container:expandable>
						</layout:cell>
					</layout:row>
				</layout:table>
			</layout:cell>
			<layout:cell>
				<container:transparent id="xml" height="600" width="625">
					<view:body href="views/container/expandable/expandableComment.xml" />
				</container:transparent>
			</layout:cell>
		</layout:row>
	</layout:table>

</worksheet>