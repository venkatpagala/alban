<?xml version="1.0" encoding="UTF-8"?>
<worksheet version="1.0" name="Expandable" xmlns:container="http://www.rfs.com/fw30/container" xmlns:view="http://www.rfs.com/fw30/view" xmlns:layout="http://www.rfs.com/fw30/layout">
	
	<layout:table>
		<layout:row>
			<layout:cell>
				
				<container:static id="containers.fieldline" height="150" width="600">
					<view:view>
						<view:title>Fieldline</view:title>
						<view:body href="views/container/fieldline/fieldline.xml"/>
					</view:view>
				</container:static>
				
				<container:static id="containers.fieldlineHeight" height="150" width="600">
					<view:view>
						<view:title>Fieldline Height (75px)</view:title>
						<view:body href="views/container/fieldline/fieldlineHeight.xml"/>
					</view:view>
				</container:static>
				
				<container:static id="containers.fieldlineSpacer" height="150" width="600">
					<view:view>
						<view:title>Fieldline Spacer (5px)</view:title>
						<view:body href="views/container/fieldline/fieldlineSpacer.xml"/>
					</view:view>
				</container:static>
						
			</layout:cell>
		</layout:row>
	</layout:table>
	
</worksheet>